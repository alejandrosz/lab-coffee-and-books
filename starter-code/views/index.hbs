<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyC1J1p1X47TBx53q1TvRZWIxYkJZfoKfEQ">
</script>
<script type="text/javascript" src="/javascripts/main.js"></script>

<div class="father">
  <h1>cafe y libros</h1>
  <div id="map"></div>
</div>

{{#each placesFound}}
<div class="places card">
  <a href="/places/{{_id}}">
    <h3>{{name}}</h3>
  </a>
  <h3>{{type}}</h3>
  <form action="/places/{{_id}}/delete" method="POST">
    <button>delete</button>
  </form>
</div>
{{/each}}

<form class="create-form" action="/places" method="POST">
  <input type="text" name="name" placeholder="name">
  <select name="type">
    <option value="Cofee Shop">Cofee Shop</option>
    <option value="Bookstore">Bookstore</option>
  </select>
  <button>create</button>
</form>